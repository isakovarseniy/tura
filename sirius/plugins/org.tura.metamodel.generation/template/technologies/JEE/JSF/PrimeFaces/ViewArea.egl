[%@template
operation form::ViewArea draw(hash) { 
    var util = new Native('org.tura.metamodel.commons.Util');
     util.traceIfDebug("UI element","ViewArea start: "+self.name);  
     if (self.baseCanvas == null){
       return;
     }
        
%]
       [%=self.baseCanvas.draw(hash)%]
[%
     util.traceIfDebug("UI element","ViewArea end: "+self.name);     
}
%]