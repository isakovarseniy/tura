@template
operation   form::Uielement  refreshArias() {
   var update = self.refreshAreas->select(r|r.group > 1);
   var process = self.refreshAreas->select(r|r.group == 1)->sortBy(q|q.group);
     %]process="[%=refreshAriasGroup(process, self)%]"  update="[%=refreshAriasGroup(update,self)%]"[%
}


operation  form::Uielement isRefreshArias() {
   var update = self.refreshAreas->select(r|r.group > 1);
   var process = self.refreshAreas->select(r|r.group == 1)->sortBy(q|q.group);
   if (update.size() == 0 and process.size() == 0){
      return false;
   }
   return true;
   
}


operation refreshAriasGroup(list, obj ){
     var result ="";
     var uid = obj.mkuid_();
     
    if (list.size() ==  0 ) return result;
    else{
      var first = 0;
      for ( ref in list ){
         if ( first = 1){
           result = result +",";
         }
         var refaria = ref.area.mkuid_();
         if  (uid == refaria){
            result = result+"@this";
         }else{
             result = result+"#{viewIdentificator.getClientId("+refaria+")}";
         }
         
         first=1;
      }
    }
    return result;
}

