<?xml version="1.0"?>
<project
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.tura</groupId>
		<artifactId>test-java</artifactId>
		<version>2.0</version>
	</parent>

	<groupId>org.tura.test-java</groupId>
	<artifactId>commons-test</artifactId>

	<name>Tura :: Platform :: Java :: Test :: Commons</name>
	<url>http://sourceforge.net/p/tura/wiki/Home/</url>


	<dependencies>

		<dependency>
			<groupId>org.hibernate.javax.persistence</groupId>
			<artifactId>hibernate-jpa-2.1-api</artifactId>
		</dependency>

		<dependency>
			<groupId>org.tura.platform-java.datacontrol</groupId>
			<artifactId>core</artifactId>
		</dependency>

		<dependency>
			<groupId>org.tura.platform-java.datacontrol</groupId>
			<artifactId>annotations</artifactId>
		</dependency>


		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
		</dependency>

		<dependency>
			<groupId>org.tura.platform.test.generated-code</groupId>
			<artifactId>hr-model-interface</artifactId>
			<version>1.0</version>
		</dependency>


		<dependency>
			<groupId>com.h2database</groupId>
			<artifactId>h2</artifactId>
		</dependency>

		<dependency>
			<groupId>javax</groupId>
			<artifactId>javaee-api</artifactId>
			<scope>provided</scope>
		</dependency>



	</dependencies>


	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.19.1</version>
				<configuration>
					<skipTests>true</skipTests>
				</configuration>
			</plugin>
		</plugins>
	</build>


</project>
