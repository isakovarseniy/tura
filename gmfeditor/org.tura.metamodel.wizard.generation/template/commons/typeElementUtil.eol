operation TypeElement getMappedJavaType( recipe:Recipe, ingredient :Ingredient) :  Mapper { 

    var appmappers =   new Native('java.util.ArrayList');  
    var util = new Native('org.tura.metamodel.commons.Util');
    
    appmappers.addAll(recipe.mappers);
    appmappers.addAll(ingredient.mappers);
    
    for( appmapper in appmappers ){
          var set = appmapper.mapper.mappers.select(r|r.typeRef.uid=self.uid);
          return util.mapType(set,ingredient,self) ;
     }
    
} 
