wizard PopulateGenerationSequence{
	
	guard : self.isKindOf(domain::ModelMapper) 
		
	title : 'Populate Generation Sequence'
	
	do {
	   var uuid := Native('java.util.UUID');
       var recipe = self.parent.parent.parent;

       if (recipe.deployment =null){
          var ds = new DeploymentSequence;
           ds.name="Deployment sequence";
           ds.uid=uuid.randomUUID().toString();
           recipe.deployment = ds;
           recipe.parent.deployment=ds;
       }
       
       if (recipe.deployment.deploymentComponents =null){
           recipe.deployment.deploymentComponents  = new DeploymentComponents;
           recipe.deployment.deploymentComponents.uid = uuid.randomUUID().toString();
       }

       if (recipe.deployment.deploymentComponents.startSeq =null){
            recipe.deployment.deploymentComponents.startSeq  = new DeploymentStarStep;
            recipe.deployment.deploymentComponents.startSeq.uid = uuid.randomUUID().toString();
       }
	   
	   var mapper=recipe.deployment.deploymentComponents.deplymentStep.selectOne(r|r.deploymentComponent=self.uid);
	   if (mapper=null){
	       var deploymentComponent = new DeploymentComponent;
           deploymentComponent.uid=uuid.randomUUID().toString();
	       deploymentComponent.mapper=self;
	       deploymentComponent.name=self.parent.parent.name+":"+self.parent.name+":"+self.name;
	    
	      recipe.deployment.deploymentComponents.deplymentStep.add(deploymentComponent);
	   }
	
	}
	
}
