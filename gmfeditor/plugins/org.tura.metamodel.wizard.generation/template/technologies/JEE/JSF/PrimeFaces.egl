operation   domain::Uielement  refreshArias() {
   var update = self.refreshAreas->select(r|r.group > 1);
   var process = self.refreshAreas->select(r|r.group == 1)->sortBy(q|q.group);


    if (update.size() ==  0 ) return;
    else{
     %]update="[%
      var first = 0;
      for ( ref in update ){
         if ( first = 1){
           %],[% 
         }
         %]#{viewmodel.getClientId([%=ref.area.mkuid()%])}[%  
         first=1;
      }
    }%]"[%
    
    if (process.size() ==  0 ) return;
    else{
     %] process="[%
      var first = 0;
      for ( ref in process ){
         if ( first = 1){
           %],[% 
         }
         %]#{viewmodel.getClientId([%=ref.area.mkuid()%])}[%  
         first=1;
      }
    }%]"[%
}
