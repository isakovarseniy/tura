            constraints="rules/runtimeTypedefinitionValidationRules.evl"
            namespaceURI="http://tura.org/2013/v1/domain"
            runtime="true">
      </constraintsBinding>
   </extension>
   

    <extension
         point="org.eclipse.ui.ide.markerResolution">
