 context Package {

  constraint HasName {

      check : self.name.isDefined()

      message : 'Unnamed ' + self.eClass().name + ' not allowed'

  }

   constraint UniqueName{

      guard : self.satisfies("HasName")
      check : Package.all.select(r|r.name=self.name)->size() = 1
      message : 'Package name  ' + self.name + ' is not unique'
   
   } 

}

