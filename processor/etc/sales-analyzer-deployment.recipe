tura:install chromedriver

mr2 = tura:dsl config --mavenRepositoryLocation

tura:docker stopContainer --name sa-wf

tura:dsl removeFile --targetLocation $tura_working_directory/resources/sa/deployments/standalone/deployments


tura:dsl jbossDoDeploy\
         --jboss_home $tura_working_directory/resources/sa/deployments\
         --targetName kie-server.war\
         --srcResource $mr2/org/kie/server/kie-server/6.5.0.Final/kie-server-6.5.0.Final-ee7.war


tura:dsl copyFile\
         --targetLocation $tura_working_directory/resources/sa/deployments/standalone/deployments/kie-server.war/WEB-INF/lib/\
         --targetName server-extension-${tura_version}.jar\
         --sourceResource "$mr2/sales-analyzer/server-extension/${tura_version}/server-extension-${tura_version}.jar" 


tura:dsl copyFile\
         --targetLocation $tura_working_directory/resources/sa/deployments/standalone/deployments/kie-server.war/WEB-INF/lib/\
         --targetName api-extension-${tura_version}.jar\
         --sourceResource $mr2/sales-analyzer/api-extension/${tura_version}/api-extension-${tura_version}.jar


tura:dsl copyFile\
         --unzip\
         --sourceResource $tura_home/examples/sales-analyzer/talend-jobs/monthly_file_processing.zip\
         --targetLocation $tura_working_directory/resources/sa/jobs\
         --targetName monthly_file_processing.zip

