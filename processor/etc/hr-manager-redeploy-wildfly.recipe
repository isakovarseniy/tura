mr2 = tura:dsl config --mavenRepositoryLocation

tura:docker stopContainer --name hr-wf

tura:dsl removeFile --targetLocation $tura_working_directory/resources/hrmanager/deployments
tura:dsl mkDir --targetLocation $tura_working_directory/resources/hrmanager/deployments

tura:dsl jbossDoDeploy --jboss_home $tura_working_directory/resources/hrmanager/deployments\
                                      --targetName hrform-react-${tura_version}.war\
                                      --srcResource $mr2/org/tura/example/ui/hrform-react/${tura_version}/hrform-react-${tura_version}.war


tura:dsl jbossDoDeploy --jboss_home $tura_working_directory/resources/hrmanager/deployments\
                                      --targetName hr-form-react-client-${tura_version}.war\
                                      --srcResource $mr2/org/tura/example/ui/hr-form-react-client/${tura_version}/hr-form-react-client-${tura_version}.war


tura:docker startContainer --name hr-wf
